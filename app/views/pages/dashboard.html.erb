<h1>Dashboard</h1>

<ul>
  <% @bookings.each do |booking| %>
    <li>
      <%= booking.user_id %>
      <%= booking.dream.title %>
      <%= booking.reservation_date %>
    </li>
  <% end %>
</ul>

<ul>
  <% @dreams.each do |dream| %>
    <li>
      <%= dream.user_id %>
      <%= dream.title %>
    </li>
  <% end %>
</ul>
